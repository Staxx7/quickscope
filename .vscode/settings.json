{
    "css.validate": false,
    "scss.validate": false,
    "less.validate": false,
    "css.customData": [".vscode/css_custom_data.json"],
    "files.associations": {
      "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
      "strings": true
    },
    "tailwindCSS.includeLanguages": {
      "typescript": "typescript",
      "typescriptreact": "typescriptreact"
    },
    "tailwindCSS.experimental.classRegex": [
      "class[:]\\s*['\"]([^'\"]*)['\"]"
    ]
  }
  